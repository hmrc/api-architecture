{
  "$id": "https://www.gov.uk/government/organisations/hm-revenue-customs/schema/itsa/Customer_Defined_Employment",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Customer Defined Employment Reponse",
  "description": "Object representing a customer defined employment",
  "type": "object",
  "additionalProperties": false,
  "definitions": {
    "employmentId": {
      "id": "#employmentId",
      "description": "Unique identifier of the employment",
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$",
      "format": "uuid"
    },
  "required": ["employmentId"],
  "properties": {
    "employmentID": { "type": "string", "pattern": "^([0-9]{3})/([^ ].{0,10})$"}
  },
  "properties": {
    "submittedOn": {
      "type": "string",
      "format": "date-time"
    },

  }